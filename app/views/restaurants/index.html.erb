
<div class="container">
  <div class="row">
    <div class="col-12">
      <h1>My list of restaurants</h1>
      <% @restaurants.each do |restaurant| %>
      <h2><%=link_to restaurant.name, restaurant_path(restaurant) %></h2>
      <p><%= restaurant.address %></p>
      <% end %>
    </div>
  </div>
</div>
